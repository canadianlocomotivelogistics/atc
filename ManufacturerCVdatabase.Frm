Private Sub Command1_Click()

Dim NewWs As Workspace
Dim NewDb As Database
Dim myTable As TableDef

Set NewWs = DBEngine.Workspaces(0)
Set NewDb = NewWs.CreateDatabase(App.Path$ & "\Databases\Test.mdb", dbLangGeneral)
Set myTable = NewDb.CreateTableDef("DecoderList")

Set myf1 = myTable.CreateField("RecordNumber", dbLong)
'Let myf1.AllowZeroLength = False ' Operation not Valid
Let myf1.Attributes = dbUpdatableField + dbAutoIncrField

Set myF2 = myTable.CreateField("Manufacturer", dbText)
Let myF2.AllowZeroLength = True
Let myF2.Attributes = dbUpdatableField
Let myF2.DefaultValue = "'No manufacturer entered.'"

Set myF3 = myTable.CreateField("Decoder", dbText)
Let myF3.AllowZeroLength = True
Let myF3.Attributes = dbUpdatableField
Let myF3.DefaultValue = "'No decoder type entered.'"

myTable.Fields.Append myf1
myTable.Fields.Append myF2
myTable.Fields.Append myF3

NewDb.TableDefs.Append myTable

NewDb.Close

If CheckBoxLink.Value = vbChecked Then Call CV1_Click

End Sub


